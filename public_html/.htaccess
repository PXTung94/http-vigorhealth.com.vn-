RewriteEngine On
Options +FollowSymlinks 
RewriteBase /
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

RewriteRule ^english$ index.php?page=page-home&lang=eng
