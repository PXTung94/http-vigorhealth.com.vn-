RewriteRule ^tin-tuc/([^/]*)$ index.php?page=page-news&sckind=1&scnewsid=$1
RewriteRule ^tin-tuc/([^/]*)/([0-9]*)$ index.php?page=page-news&sckind=1&scnewsid=$1&pz=$2
RewriteRule ^tin-tuc/([^/]*)/([^/]*)$ index.php?page=page-news-view&sckind=1&scnewsid=$1&snewsid=$2

RewriteRule ^dien-dan$ index.php?page=page-faq&sckind=7
RewriteRule ^dien-dan/([0-9]*)$ index.php?page=page-faq&pz=$1&sckind=7
RewriteRule ^dien-dan/([^/]*)$ index.php?page=page-faq-reply&snewsid=$1&sckind=7

RewriteRule ^xem-ho-so$ page-login.php
RewriteRule ^dat-cau-hoi$ index.php?page=page-faq-ask#askFaq

RewriteRule ^lien-he$ index.php?page=page-contact
